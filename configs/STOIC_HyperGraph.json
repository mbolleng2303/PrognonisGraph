{
    "gpu": {
        "use": true,
        "id": 0
    },

    "model": "STOIC_Hyper",
    "dataset": "STOIC",

    "out_dir": "./out/",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 1,
        "init_lr": 0.005,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 21,
        "min_lr": 5e-7,
        "weight_decay": 0.0005,
        "print_epoch_interval": 5,
        "max_time": 12,
        "cross_val": true,
        "split_num": 0
    },
    "net_params": {
        "L":3,
        "n_transformers" : 4,
        "n_heads" : 4,
        "cls_token": true,
        "feature_dim": 256,
        "similarity_dim": 256,
        "mlp_dim": 256,
        "residual": true,
        "in_feat_dropout": 0.3744,
        "dropout": 0.2755,
        "batch_norm": true,
        "sage_aggregator": "mean",
        "edge_feat": false
    }
}