        "residual": true,
        "in_feat_dropout": 0.4,
        "dropout": 0.5863,
        "batch_norm": true,
        "sage_aggregator": "mean",
        "edge_feat": false,
        "lambda1": 0.9,
        "lambda2": 1.0,
        "lambda3": 1.0,
        "lambda4": 1.0,
        "lambda5": 0.8,
        "lambda6": 0.0
