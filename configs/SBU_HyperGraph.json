{
    "gpu": {
        "use": true,
        "id": 1
    },
    "model": "SBU_Hyper",
    "dataset": "SBU",
    "out_dir": "./out/",
    "params": {
        "seed": 41,
        "epochs": 10000,
        "batch_size": 1,
